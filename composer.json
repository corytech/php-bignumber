{
  "name": "corytech/php-bignumber",
  "type": "library",
  "description": "BigNumber library",
  "keywords": ["bignumber"],
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.49",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Corytech\\BigNumber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Corytech\\BigNumber\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}